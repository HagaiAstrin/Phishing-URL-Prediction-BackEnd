{
  "manifest_version": 3,
  "name": "Phishing URL Checker",
  "version": "1.0",
  "description": "Check if a URL is phishing or not",
  "permissions": [],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "Photos/icon16.png",
      "48": "Photos/icon48.png",
      "128": "Photos/icon128.png"
    }
  },
  "icons": {
    "16": "Photos/icon16.png",
    "48": "Photos/icon48.png",
    "128": "Photos/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["xgb_model.json"],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": ["<all_urls>"]
}
